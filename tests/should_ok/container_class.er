D = Class { Int: [Str; _] }
_ = D.new {:}
_ = D.new {1: ["a"]}

D! = Class Dict! { Int: [Str; _] }
_ = D!.new !{:}
_ = D!.new !{1: ["a"]}

D2 = Class { Str: Array[Int] }
_ = D2.new { "a": [1] }

D3 = Class { Str: Array![Int] }
_ = D3.new { "a": ![1] }

D4! = Class Dict! { Str: Array![Int] }
_ = D4!.new !{ "a": ![1] }
