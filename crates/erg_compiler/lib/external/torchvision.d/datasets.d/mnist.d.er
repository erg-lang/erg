vision = pyimport "./vision"

.MNIST: ClassType
.MNIST <: vision.VisionDataset
.MNIST.
    __new__: (
        cls: ClassType,
        root: Str,
        train := Bool,
        download := Bool,
        transform := GenericCallable,
        target_transform := GenericCallable,
    ) -> .MNIST

.FashionMNIST: ClassType
.FashionMNIST <: .MNIST
.FashionMNIST.
    __new__: (
        cls: ClassType,
        root: Str,
        train := Bool,
        download := Bool,
        transform := GenericCallable,
        target_transform := GenericCallable,
    ) -> .FashionMNIST
