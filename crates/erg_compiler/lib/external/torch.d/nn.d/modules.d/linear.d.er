{Module;} = pyimport "torch/nn"
{Device; DType;} = pyimport "torch"

.Linear: ClassType
.Linear <: Module
.Linear <: GenericCallable
.Linear.
    __new__: (
        cls: ClassType,
        in_features: Nat,
        out_features: Nat,
        bias := Bool,
        device := Device or Str or Nat,
        dtype := DType or Str,
    ) -> .Linear
