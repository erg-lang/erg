.BaseLoader: ClassType

.PackageLoader: ClassType
.PackageLoader <: .BaseLoader
.PackageLoader.
    __new__: (cls: ClassType, package_name: Str, package_path: Str) -> .PackageLoader

.FileSystemLoader: ClassType
.FileSystemLoader <: .BaseLoader
.FileSystemLoader.
    __new__: (cls: ClassType, path: Str) -> .FileSystemLoader

.DictLoader: ClassType
.DictLoader <: .BaseLoader
.DictLoader.
    __new__: (cls: ClassType, mapping: {Str: Str}) -> .DictLoader

.Template: ClassType
.Template.
    __new__: (cls: ClassType, source: Str, autoescape := Bool, enable_async := Bool) -> .Template
    render: (self: Ref(.Template), **kwargs: Obj) -> Str

.Enviroment: ClassType
.Enviroment.
    __new__: (cls: ClassType, loader := .BaseLoader, trim_blocks := Bool) -> .Enviroment
    get_template: (self: Ref(.Enviroment), name := Str) -> .Template
